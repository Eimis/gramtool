# Dalyviai

@rule dalyvis (nustatyti)
V     . i     #     nustatyti
tmsns . as    # kas nustatytas
tmsgs . o     # ko  nustatyto   
tmsds . am    # kam nustatytam  
tmsas . ą     # ką  nustatytą   
tmsbs . u     # kuo nustatytu   
tmsls . ame   # kur nustatytame 
tmpns . i     # kas nustatyti   
tmpgs . ų     # ko  nustatytų   
tmpds . iems  # kam nustatytiems
tmpas . us    # ką  nustatytus  
tmpbs . ais   # kuo nustatytais 
tmpls . uose  # kur nustatytuose

# Padalyviai

@rule padalyvis (laukiant)
V     . ti        # laukti
gp    . iant      # laukiant
gs    . us        # laukus
gi    . davus     # laukdavus
gf    . siant     # lauksiant

@rule padalyvis (išskyrus)
V     . irti      # išskirti
gp    . iriant    # išskiriant
gs    . yrus      # išskyrus
gi    . irdavus   # išskirdavus
gf    . irsiant   # išskirsiant

@rule padalyvis (einant)
V     . eiti      # eiti
gp    . einant    # einant
gs    . ėjus      # ėjus
gi    . eidavus   # eidavus
gf    . eisiant   # eisiant

@rule padalyvis (žiūrint)
V     . ėti       # žiūrėti
gp    . int       # žiūrint
gs    . ėjus      # žiūrėjus
gi    . ėdavus    # žiūrėdavus
gf    . ėsiant    # žiūrėsiant

@rule padalyvis (įskaitant)
