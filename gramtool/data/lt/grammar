# Lithuanian grammar rules

@rule veiksmažodis -u,-i (dirbti)
V     . ti       #     dirbti
vip1s . u        # aš  dirbu
vip2s . i        # tu  dirbi
vip3s . a        # jis dirba
vip1p . ame      # mes dirbame
vip2p . ate      # jūs dirbate
vip3p . a        # jie dirba
vis1s . au       # aš  dirbau
vis2s . ai       # tu  dirbai
vis3s . o        # jis dirbo
vis1p . ome      # mes dirbome
vis2p . ote      # jūs dirbote
vis3p . o        # jie dirbo
viP1s . davau    # aš  dirbdavau
viP2s . davai    # tu  dirbdavai
viP3s . davo     # jis dirbdavo
viP1p . davome   # mes dirbdavome
viP2p . davote   # jūs dirbdavote
viP3p . davo     # jie dirbdavo
vif1s . siu      # aš  dirbsiu
vif2s . si       # tu  dirbsi
vif3s . s        # jis dirbs
vif1p . sime     # mes dirbsime
vif2p . site     # jūs dirbsite
vif3p . s        # jie dirbs
vs-1s . čiau     # aš  dirbčiau
vs-2s . tum      # tu  dirbtum
vs-3s . tų       # jis dirbtų
vs-1p . tume     # mes dirbtume
vs-2p . tumėte   # jūs dirbtumėte
vs-3p . tų       # jie dirbtų
vmp2s . k        # tu  dirbk
vmp2p . kit      # jūs dirbkit
vmp3p . kite     # jūs dirbkite

@rule A -oti miegoti
V     . oti   #     mieg-oti
vip1s . u   # aš  mieg-u
vip2s . i   # tu  mieg-i
vip3s . a   # jis mieg-a
vip1p . ame   # mes mieg-ame
vip2p . ate   # jūs mieg-ate
vip3p . a   # jie mieg-a
vis1s . ojau   # aš  mieg-ojau
vis2s . ojai   # tu  mieg-ojai
vis3s . ojo   # jis mieg-pjo
vis1p . ojome   # mes mieg-ojome
vis2p . ojote   # jūs mieg-ojote
vis3p . ojo   # jie mieg-ojo
vif1s . osiu   # aš  mieg-osiu
vif2s . osi   # tu  mieg-osi
vif3s . os   # jis mieg-os
vif1p . osime   # mes mieg-osime
vif2p . osite   # jūs mieg-osite
vif3p . os   # jie mieg-os
vmp2s . ok
vmp2p . okite

@rule A -oti galvoti
V     . ti   #     galvo-
vip1s . ju   # aš  galvo-ju
vip2s . ji   # tu
vip3s . ja   # jis
vip1p . jame   # mes
vip2p . jate   # jūs
vip3p . ja   # jie
vis1s . jau   # aš
vis2s . jai   # tu
vis3s . jo   # jis
vis1p . jome   # mes
vis2p . jote   # jūs
vis3p . jo   # jie
vif1s . siu   # aš
vif2s . si   # tu
vif3s . s   # jis
vif1p . sime   # mes
vif2p . site   # jūs
vif3p . s   # jie
vmp2s . k
vmp2p . kite

@rule A -ti mėgti
V     . gti
vip1s . gstu
vip2s . gsti
vip3s . gsta
vip1p . gstame
vip2p . gstate
vip3p . gsta
vis1s . gau
vis2s . gai
vis3s . go
vis1p . gome
vis2p . gote
vis3p . go
vif1s . gsiu
vif2s . gsi
vif3s . gs
vif1p . gsime
vif2p . gsite
vif3p . gs
vmp2s . k
vmp2p . kite

@rule A -ėti
V     . ėti   #     kalbėti
vip1s . u   # aš  kalbu
vip2s . i   # tu  kalbi
vip3s . a   # jis kalba
vip1p . ame   # mes kalbame
vip2p . ate   # jūs kalbate
vip3p . a   # jie kalba
vis1s . ėjau   # aš  kalbėjau
vis2s . ėjai   # tu  kalbėjai
vis3s . ėjo   # jis kalbėjo
vis1p . ėjome   # mes kalbėjome
vis2p . ėjote   # jūs kalbėjote
vis3p . ėjo   # jie kalbėjo
vif1s . ėsiu   # aš  kalbėsiu
vif2s . ėsi   # tu  kalbėsi
vif3s . ės   # jis kalbės
vif1p . ėsime   # mes kalbėsime
vif2p . ėsite   # jūs kalbėsite
vif3p . ės   # jie kalbės
vmp2s . ėk
vmp2p . ėkite

@rule veiksmažodis, -ėti,-o,-ėjo (privalėti)
V     . ėti    #     privalėti
vip1s . au     # aš  privalau
vip2s . ai     # tu  privalai
vip3s . o      # jis privalo
vip1p . ome    # mes privalome
vip2p . ote    # jūs privalote
vip3p . o      # jie privalo
vis1s . ėjau   # aš  privalėjau
vis2s . ėjai   # tu  privalėjai
vis3s . ėjo    # jis privalėjo
vis1p . ėjome  # mes privalėjome
vis2p . ėjote  # jūs privalėjote
vis3p . ėjo    # jie privalėjo
vif1s . ėsiu   # aš  privalėsiu
vif2s . ėsi    # tu  privalėsi
vif3s . ės     # jis privalės
vif1p . ėsime  # mes privalėsime
vif2p . ėsite  # jūs privalėsite
vif3p . ės     # jie privalės
vmp2s . ėk     # tu  privalėk
vmp2p . ėkite  # jūs privalėkite

@rule A -uoti STUDIJUOTI STUDI- važiuoti važi
V     . uoti   #     studijuotiti
vip1s . uoju   # aš  studijuo-ju
vip2s . uoji   # tu  -ji
vip3s . uoja   # jis -ja
vip1p . uojame   # mes -jame
vip2p . uojate   # jūs -jate
vip3p . uoja   # jie -ja
vis1s . avau   # aš
vis2s . avai   # tu
vis3s . avo   # jis
vis1p . avome   # mes
vis2p . avote   # jūs
vis3p . avo   # jie
vif1s . uosiu   # aš
vif2s . uosi   # tu
vif3s . uos   # jis
vif1p . uosime   # mes
vif2p . uosite   # jūs
vif3p . uos   # jie
vmp2s . uok
vmp2p . uokite

@rule -AUTI PARAGAUTI
V     . uti   #     paraga-uti
vip1s . uju   # aš  studijuo-ju
vip2s . uji   # tu  -ji
vip3s . uja   # jis -ja
vip1p . ujame   # mes -jame
vip2p . ujate   # jūs -jate
vip3p . uja   # jie -ja
vis1s . vau   # aš  kalbėjau
vis2s . vai   # tu  kalbėjai
vis3s . vo   # jis kalbėjo
vis1p . vome   # mes kalbėjome
vis2p . vote   # jūs kalbėjote
vis3p . vo   # jie kalbėjo
vif1s . usiu   # aš  kalbėsiu
vif2s . usi   # tu  kalbėsi
vif3s . us   # jis kalbės
vif1p . usime   # mes kalbėsime
vif2p . usite   # jūs kalbėsite
vif3p . us   # jie kalbės
vmp2s . uk
vmp2p . ukite


@rule EITI
V     . eiti
vip1s . einu
vip2s . eini
vip3s . eina
vip1p . einame
vip2p . einate
vip3p . eina
vis1s . ėjau
vis2s . ėjai
vis3s . ėjo
vis1p . ėjome
vis2p . ėjote
vis3p . ėjo
vif1s . eisiu
vif2s . eisi
vif3s . eis
vif1p . eisime
vif2p . eisite
vif3p . eis
vmp2s . eik
vmp2p . eikite

@rule IMTI
V     . imti
vip1s . imu
vip2s . imi
vip3s . ima
vip1p . imame
vip2p . imate
vip3p . ima
vis1s . ėmiau
vis2s . ėmei
vis3s . ėmė
vis1p . ėmėme
vis2p . ėmėte
vis3p . ėmė
vif1s . imsiu
vif2s . imsi
vif3s . ims
vif1p . imsime
vif2p . imsite
vif3p . ims
vmp2s . imk
vmp2p . imkite

@rule pūsti
V     pūsti
vip1s pučiu
vip2s puti
vip3s pučia
vip1p pučiame
vip2p pučiate
vip3p pučia
vis1s pūčiau
vis2s pūtei
vis3s pūtė
vis1p pūtėme
vis2p pūtėte
vis3p pūtė
vif1s pūsiu
vif2s pūsi
vif3s pūs
vif1p pūsime
vif2p pūsite
vif3p pūs
vmp2s pūsk
vmp2p pūskite

@rule virti
V     . virti
vip1s . verdu
vip2s . verdi
vip3s . verda
vip1p . verdame
vip2p . verdate
vip3p . verda
vis1s . viriau
vis2s . virei
vis3s . virė
vis1p . virėme
vis2p . virėte
vis3p . virė
vif1s . virsiu
vif2s . virsi
vif3s . virs
vif1p . virsime
vif2p . virsite
vif3p . virs
vmp2s . virk
vmp2p . virkite

@rule padėti
V     . dėti   #     studijuotiti
vip1s . dedu   # aš  studijuo-ju
vip2s . dedi   # tu  -ji
vip3s . deda   # jis -ja
vip1p . dedame   # mes -jame
vip2p . dedate   # jūs -jate
vip3p . deda   # jie -ja
vis1s . dėjau   # aš  kalbėjau
vis2s . dėjai   # tu  kalbėjai
vis3s . dėjo   # jis kalbėjo
vis1p . dėjome   # mes kalbėjome
vis2p . dėjote   # jūs kalbėjote
vis3p . dėjo   # jie kalbėjo
vif1s . dėsiu   # aš  kalbėsiu
vif2s . dėsi   # tu  kalbėsi
vif3s . dės   # jis kalbės
vif1p . dėsime   # mes kalbėsime
vif2p . dėsite   # jūs kalbėsite
vif3p . dės   # jie kalbės
vmp2s . dėk
vmp2p . dėkite

@rule leisti
V     . sti   #     lei-sti
vip1s . džiu   # aš  lei-džiu
vip2s . di   # tu  lei-di
vip3s . džia   # jis lei-džia
vip1p . džiame   # mes lei-džiame
vip2p . džiate   # jūs lei-džiate
vip3p . džia   # jie lei-džia
vis1s . dau   # aš  lei-dau
vis2s . dai   # tu  lei-dai
vis3s . do   # jis lei-do
vis1p . dome   # mes lei-dome
vis2p . dote   # jūs lei-dote
vis3p . do   # jie lei-do
vif1s . siu   # aš  lei-siu
vif2s . si   # tu  lei-si
vif3s . s   # jis lei-s
vif1p . sime   # mes lei-sime
vif2p . site   # jūs lei-site
vif3p . s   # jie lei-s

@rule I -ETI turėti / galėti
V     . ėti   #
vip1s . iu   # aš  turiu
vip2s . i   # tu  kalbi
vip3s . i   # jis kalba
vip1p . ime   # mes kalbame
vip2p . ite   # jūs kalbate
vip3p . i   # jie kalba
vis1s . ėjau   # aš  kalbėjau
vis2s . ėjai   # tu  kalbėjai
vis3s . ėjo   # jis kalbėjo
vis1p . ėjome   # mes kalbėjome
vis2p . ėjote   # jūs kalbėjote
vis3p . ėjo   # jie kalbėjo
vif1s . ėsiu   # aš  kalbėsiu
vif2s . ėsi   # tu  kalbėsi
vif3s . ės   # jis kalbės
vif1p . ėsime   # mes kalbėsime
vif2p . ėsite   # jūs kalbėsite
vif3p . ės   # jie kalbės
vmp2s . ėk
vmp2p . ėkite

@rule veiksmažodis, -ti,-ia,-ė (skelbti)
V     . ti     #     skelbti
vip1s . iu     # aš  skelbiu
vip2s . i      # tu  skelbi
vip3s . ia     # jis skelbia
vip1p . iame   # mes skelbiame
vip2p . iate   # jūs skelbiate
vip3p . ia     # jie skelbia
vis1s . iau    # aš  skelbiau
vis2s . ei     # tu  skelbei
vis3s . ė      # jis skelbė
vis1p . ėme    # mes skelbėme
vis2p . ėte    # jūs skelbėte
vis3p . ė      # jie skelbė
vif1s . siu    # aš  skelbsiu
vif2s . si     # tu  skelbsi
vif3s . s      # jis skelbs
vif1p . sime   # mes skelbsime
vif2p . site   # jūs skelbsite
vif3p . s      # jie skelbs
vmp2s . k      # tu  skelbk
vmp2p . kite   # jės skelbkite

@rule veiksmažodis, -ti,-ia,-ė (laukti)
V     . ti     #     laukti
vip1s . iu     # aš  laukiu
vip2s . i      # tu  lauki
vip3s . ia     # jis laukia
vip1p . iame   # mes laukiame
vip2p . iate   # jūs laukiate
vip3p . ia     # jie laukia
vis1s . iau    # aš  laukiau
vis2s . ei     # tu  laukei
vis3s . ė      # jis laukė
vis1p . ėme    # mes laukėme
vis2p . ėte    # jūs laukėte
vis3p . ė      # jie laukė
vif1s . siu    # aš  lauksiu
vif2s . si     # tu  lauksi
vif3s . s      # jis lauks
vif1p . sime   # mes lauksime
vif2p . site   # jūs lauksite
vif3p . s      # jie lauks
vmp2s . .      # tu  lauk
vmp2p . ite    # jės laukite


@rule veiksmažodis -yti,-au (rašyti)
V     . yti   #     rašyti
vip1s . au    # aš  rašau
vip2s . ai    # tu  rašai
vip3s . o     # jis rašo
vip1p . ome   # mes rašome
vip2p . ote   # jūs rašote
vip3p . o     # jie rašo
vis1s . iau   # aš  rašiau
vis2s . ei    # tu  rašei
vis3s . ė     # jis rašė
vis1p . ėme   # mes rašėme
vis2p . ėte   # jūs rašėte
vis3p . ė     # jie rašė
vif1s . ysiu  # aš  rašysiu
vif2s . ysi   # tu  rašysi
vif3s . ys    # jis rašys
vif1p . ysiu  # mes rašysime
vif2p . ysi   # jūs rašysite
vif3p . ys    # jie rašys
vmp2s . yk
vmp2p . ykite

@rule veiksmažodis -yti,-žiau (vykdyti)
V     . yti   #     vykdyti
vip1s . au    # aš  vykdau
vip2s . ai    # tu  vykdai
vip3s . o     # jis vykdo
vip1p . ome   # mes vykdome
vip2p . ote   # jūs vykdote
vip3p . o     # jie vykdo
vis1s . žiau  # aš  vykdžiau
vis2s . ei    # tu  vykdei
vis3s . ė     # jis vykdė
vis1p . ėme   # mes vykdėme
vis2p . ėte   # jūs vykdėte
vis3p . ė     # jie vykdė
vif1s . ysiu  # aš  vykdysiu
vif2s . ysi   # tu  vykdysi
vif3s . ys    # jis vykdys
vif1p . ysiu  # mes vykdysime
vif2p . ysi   # jūs vykdysite
vif3p . ys    # jie vykdys
vmp2s . yk    # tu  vykdyk
vmp2p . ykite # jūs vykdykite

@rule veiksmažodis -tyti,-čiau (skaityti, matyti)
V     . tyti   #     matyti
vip1s . tau    # aš  matau
vip2s . tai    # tu  matai
vip3s . to     # jis mato
vip1p . tome   # mes matome
vip2p . tote   # jūs matote
vip3p . to     # jie mato
vis1s . čiau   # aš  matčiau
vis2s . tei    # tu  matei
vis3s . tė     # jis matė
vis1p . tėme   # mes matėme
vis2p . tėte   # jūs matėte
vis3p . tė     # jie matė
vif1s . tysiu  # aš  matysiu
vif2s . tysi   # tu  matysi
vif3s . tys    # jis matys
vif1p . tysime # mes matysime
vif2p . tysite # jūs matysite
vif3p . tys    # jie matys
vmp2s . tyk    # tu  matyk
vmp2p . tykite # jūs matykite

@rule  BIJOTI
V     . oti   #     bijoti
vip1s . au   # aš  bijau
vip2s . ai   # tu  bijai
vip3s . o   # jis bijo
vip1p . ome   # mes bijome
vip2p . ote   # jūs bijote
vip3p . o   # jie bijo
vis1s . ojau   # aš  bijojau
vis2s . ojai   # tu  bijojai
vis3s . ojo   # jis bijojo
vis1p . ojome   # mes bijojome
vis2p . ojote   # jūs bijojote
vis3p . ojo   # jie bijojo
vif1s . osiu   # aš  bijosiu
vif2s . osi   # tu  bijosi
vif3s . os   # jis bijos
vif1p . osime   # mes bijosime
vif2p . osite   # jūs bijosite
vif3p . os   # jie bijos

@rule  imti / priimti
V     . imti   #
vip1s . imu   # aš  bijau
vip2s . imi   # tu  bijai
vip3s . ima   # jis bijo
vip1p . imame   # mes bijome
vip2p . imate   # jūs bijote
vip3p . ima   # jie bijo
vis1s . ėmiau   # aš  bijojau
vis2s . ėmei   # tu  bijojai
vis3s . ėmė   # jis bijojo
vis1p . ėmėme   # mes bijojome
vis2p . ėmėte   # jūs bijojote
vis3p . ėmė   # jie bijojo
vif1s . imsiu   # aš  bijosiu
vif2s . imsi   # tu  bijosi
vif3s . ims   # jis bijos
vif1p . imsime   # mes bijosime
vif2p . imsite   # jūs bijosite
vif3p . ims   # jie bijos
vmp2s . imk
vmp2p . imkite

@rule REIKŠTI
