# Lithuanian grammar rules

@rule dirbti
# Bendratis (infinitive)
V . ti   #     dirbti

# Esamasis laikas (simple present)
vip1s . u   # aš  dirbu
vip2s . i   # tu  dirbi
vip3s . a   # jis dirba
vip1p . ame   # mes dirbame
vip2p . ate   # jūs dirbate
vip3p . a   # jie dirba

# Būtasis kartinis laikas (past simple)
vis1s . au   # aš  dirbau
vis2s . ai   # tu  dirbai
vis3s . o   # jis dirbo
vis1p . ome   # mes dirbome
vis2p . ote   # jūs dirbote
vis3p . o   # jie dirbo

# Būtasis dažninis laikas (past perfect progressive)
viP1s . davau   # aš  dirbdavau
viP2s . davai   # tu  dirbdavai
viP3s . davo   # jis dirbdavo
viP1p . davome   # mes dirbdavome
viP2p . davote   # jūs dirbdavote
viP3p . davo   # jie dirbdavo

# Būsimasis laikas (future simple)
vif1s . siu   # aš  dirbsiu
vif2s . si   # tu  dirbsi
vif3s . s   # jis dirbs
vif1p . sime   # mes dirbsime
vif2p . site   # jūs dirbsite
vif3p . s   # jie dirbs

# Tariamoji nuosaka (subjunctive mood)
vs-1s . čiau   # aš  dirbčiau
vs-2s . tum   # tu  dirbtum
vs-3s . tų   # jis dirbtų
vs-1p . tume   # mes dirbtume
vs-2p . tumėte   # jūs dirbtumėte
vs-3p . tų   # jie dirbtų

# Liepiamoji nuosaka (imperative mood)
vmp2s . k   # tu  dirbk
vmp2p . kit   # jūs dirbkit
vmp3p . kite   # jūs dirbkite

@rule A -oti miegoti
V     . oti   #     mieg-oti
vip1s . u   # aš  mieg-u
vip2s . i   # tu  mieg-i
vip3s . a   # jis mieg-a
vip1p . ame   # mes mieg-ame
vip2p . ate   # jūs mieg-ate
vip3p . a   # jie mieg-a
vis1s . ojau   # aš  mieg-ojau
vis2s . ojai   # tu  mieg-ojai
vis3s . ojo   # jis mieg-pjo
vis1p . ojome   # mes mieg-ojome
vis2p . ojote   # jūs mieg-ojote
vis3p . ojo   # jie mieg-ojo
vif1s . osiu   # aš  mieg-osiu
vif2s . osi   # tu  mieg-osi
vif3s . os   # jis mieg-os
vif1p . osime   # mes mieg-osime
vif2p . osite   # jūs mieg-osite
vif3p . os   # jie mieg-os
vmp2s . ok
vmp2p . okite

@rule A -oti galvoti
V     . ti   #     galvo-
vip1s . ju   # aš  galvo-ju
vip2s . ji   # tu
vip3s . ja   # jis
vip1p . jame   # mes
vip2p . jate   # jūs
vip3p . ja   # jie
vis1s . jau   # aš
vis2s . jai   # tu
vis3s . jo   # jis
vis1p . jome   # mes
vis2p . jote   # jūs
vis3p . jo   # jie
vif1s . siu   # aš
vif2s . si   # tu
vif3s . s   # jis
vif1p . sime   # mes
vif2p . site   # jūs
vif3p . s   # jie
vmp2s . k
vmp2p . kite

@rule A -ti mėgti
V     . gti
vip1s . gstu
vip2s . gsti
vip3s . gsta
vip1p . gstame
vip2p . gstate
vip3p . gsta
vis1s . gau
vis2s . gai
vis3s . go
vis1p . gome
vis2p . gote
vis3p . go
vif1s . gsiu
vif2s . gsi
vif3s . gs
vif1p . gsime
vif2p . gsite
vif3p . gs
vmp2s . k
vmp2p . kite

@rule A -ėti
V     . ėti   #     kalbėti
vip1s . u   # aš  kalbu
vip2s . i   # tu  kalbi
vip3s . a   # jis kalba
vip1p . ame   # mes kalbame
vip2p . ate   # jūs kalbate
vip3p . a   # jie kalba
vis1s . ėjau   # aš  kalbėjau
vis2s . ėjai   # tu  kalbėjai
vis3s . ėjo   # jis kalbėjo
vis1p . ėjome   # mes kalbėjome
vis2p . ėjote   # jūs kalbėjote
vis3p . ėjo   # jie kalbėjo
vif1s . ėsiu   # aš  kalbėsiu
vif2s . ėsi   # tu  kalbėsi
vif3s . ės   # jis kalbės
vif1p . ėsime   # mes kalbėsime
vif2p . ėsite   # jūs kalbėsite
vif3p . ės   # jie kalbės
vmp2s . ėk
vmp2p . ėkite

@rule A -uoti STUDIJUOTI STUDI- važiuoti važi
V     . uoti   #     studijuotiti
vip1s . uoju   # aš  studijuo-ju
vip2s . uoji   # tu  -ji
vip3s . uoja   # jis -ja
vip1p . uojame   # mes -jame
vip2p . uojate   # jūs -jate
vip3p . uoja   # jie -ja
vis1s . avau   # aš
vis2s . avai   # tu
vis3s . avo   # jis
vis1p . avome   # mes
vis2p . avote   # jūs
vis3p . avo   # jie
vif1s . uosiu   # aš
vif2s . uosi   # tu
vif3s . uos   # jis
vif1p . uosime   # mes
vif2p . uosite   # jūs
vif3p . uos   # jie
vmp2s . uok
vmp2p . uokite

@rule -AUTI PARAGAUTI
V     . uti   #     paraga-uti
vip1s . uju   # aš  studijuo-ju
vip2s . uji   # tu  -ji
